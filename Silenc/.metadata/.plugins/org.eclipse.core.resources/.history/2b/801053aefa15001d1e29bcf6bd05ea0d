package com.GupiGames.FSM;

import java.awt.Graphics;

import com.GupiGames.main.UI;

public class Game_Menu implements GameState {

	public void update() {
	}

	public void render(Graphics g) {
		
		g.drawImage(UI.ButtonMask_off, 95, 49, null);
		g.drawImage(UI.ButtonMask_off, 95, 69, null);
		g.drawImage(UI.ButtonMask_off, 95, 89, null);
		g.drawImage(UI.ButtonMask_off, 95, 19, null);
		
		g.drawImage(UI.ButtonPlay, 96, 50, null);
		g.drawImage(UI.ButtonStory, 96, 70, null);
		g.drawImage(UI.ButtonCredits, 96, 90, null);
		g.drawImage(UI.ButtonExit, 96, 110, null);
	}

}
