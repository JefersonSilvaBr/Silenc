package com.GupiGames.World;

import java.awt.image.BufferedImage;

import com.GupiGames.main.Game;

public class FloorTile extends Tile{
	
	public static BufferedImage FloorTile = Game.tilesSheet.getSprite(16, 48, 16, 16);
	public static BufferedImage FloorTile_Right = Game.tilesSheet.getSprite(16, 32, 16, 16);
	public static BufferedImage FloorTile_Left = Game.tilesSheet.getSprite(0, 32, 16, 16);

	public FloorTile(BufferedImage sprite, int x, int y, int Width, int Height) {
		super(sprite, x, y, Width, Height);
	}

}
